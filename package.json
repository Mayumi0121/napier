{
  "name": "napier",
  "version": "0.0.0",
  "description": "simple web proxy service",
  "main": "electron/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-production": "webpack -c --mode production",
    "webbuild": "webpack -c webpack.config.js --mode development",
    "watch": "NODE_OPTIONS=--openssl-legacy-provider webpack -c webpack.config.js --watch",
    "server": "node ./server/index.js",
    "cli": "node ./client/napier-cli.js",
    "electron": "electron electron/main.js",
    "electron-build": "electron-builder"
  },
  "build": {
    "appId": "com.napier-net.electron",
    "productName": "napier",
    "files": [
      "package.json",
      "electron/main.js",
      "electron/renderer.js",
      "electron/proxy.js",
      "electron/preload.js",
      "electron/api.js",
      "electron/index.html"
    ],
    "mac":{
      "target": "dmg"
    },
    "win":{
      "target": "nsis"
    },
    "nsis":{
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "author": "Masami Ogoshi",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.2",
    "assert": "^2.0.0",
    "axios": "^0.27.2",
    "axios-cookiejar-support": "^4.0.3",
    "bcrypt": "^5.0.1",
    "bootstrap": "^5.2.0",
    "bootstrap-icons": "^1.9.1",
    "commander": "^9.4.0",
    "connect-multiparty": "^2.2.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "file-loader": "^6.2.0",
    "markdown-it": "^12.3.2",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-container": "^2.0.0",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-ins": "^2.0.0",
    "markdown-it-mark": "^2.0.0",
    "markdown-it-prism": "^2.2.1",
    "markdown-it-replace-link": "^1.1.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "mime": "^3.0.0",
    "morgan": "^1.10.0",
    "node-rsa": "^1.1.1",
    "node-uuid": "^1.4.8",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "prismjs": "^1.25.0",
    "querystring": "^0.2.1",
    "redbird": "^0.10.0",
    "sequelize": "^6.21.3",
    "serve-handler": "^6.1.3",
    "session-file-store": "^1.5.0",
    "sprightly": "^1.0.3",
    "sqlite3": "^5.0.10",
    "svelte-loader": "^3.1.3",
    "svelte-preprocess": "^4.10.7",
    "svgo-loader": "^3.0.1",
    "url-loader": "^4.1.1",
    "ws": "^8.8.0"
  },
  "devDependencies": {
    "@electron/get": "^2.0.2",
    "css-loader": "^6.7.1",
    "electrode-server": "^3.3.0",
    "electron": "^22.0.0",
    "electron-builder": "^22.10.3",
    "sequelize-cli": "^6.4.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  }
}
